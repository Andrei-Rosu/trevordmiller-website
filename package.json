{
  "name": "trevordmiller-website",
  "scripts": {
    "dev": "next",
    "test": "jest",
    "lint": "eslint src",
    "build": "next build",
    "verify": "yarn test && yarn lint && yarn build",
    "start": "next start",
    "stage": "now",
    "deploy": "now --alias"
  },
  "now": {
    "name": "trevordmiller",
    "aliases": [
      "trevordmiller.com"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "react/display-name": [
        "off"
      ],
      "react/prop-types": [
        "off"
      ]
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.10.2",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^17.0.3"
  },
  "dependencies": {
    "next": "^1.2.3",
    "nova-colors": "^2.1.5",
    "react-tabs": "^0.8.2"
  }
}
